@using WebApi02.DataModel;
@model IEnumerable<Employee>
    <div>
    <table id="Workers" class="table table-striped table-bordered table-info">
        <tr class="col-auto">
            <th class=""> id     </th>
            <th > full_name </th>
            <th > salary </th>
            <th > gender </th>
        </tr>
        @if (Model != null)
        {
            foreach (var p in Model)
            {
            <tr>
                <td> @p.Emp_Id </td>
                <td> @p.first_name @p.last_name </td>
                <td> @p.salary  </td>
                <td> @p.gender  </td>
            </tr>
            }
        }
    </table>
    </div>
 
